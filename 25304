import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        int total, t, a, b, sum = 0;

        total = scanner.nextInt();
        t = scanner.nextInt();

        for(int i = 0; i < t; i++){
            a = scanner.nextInt();
            b = scanner.nextInt();
            sum += a * b;
        }
        if(sum == total){
            System.out.println("Yes");
        }
        else{
            System.out.println("No");
        }

        scanner.close();
    }
}
