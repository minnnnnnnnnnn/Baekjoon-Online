import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        String s;
        int n;

        n = scanner.nextInt();
        for(int i = 0; i < n; i++){
            s = scanner.next();
            System.out.print(s.charAt(0));
            System.out.println(s.charAt(s.length() - 1));
        }

        scanner.close();

    }
}
