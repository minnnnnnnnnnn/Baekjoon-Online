import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        int[] chess = new int[6];
        int[] a = {1, 1, 2, 2, 2, 8};

        for(int i = 0; i < 6; i++){
            chess[i] = scanner.nextInt();
        }

        for(int i = 0; i < 6; i++) {
            System.out.print(a[i] - chess[i]);
            System.out.print(" ");
        }

        scanner.close();

    }
}
