import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);
        int fir, sec, thi;
        fir = scanner.nextInt();
        sec = scanner.nextInt();
        thi = scanner.nextInt();
        System.out.println((fir + sec) % thi);
        System.out.println(((fir % thi) + (sec % thi)) % thi);
        System.out.println((fir * sec) % thi);
        System.out.println(((fir % thi) * (sec % thi)) % thi);

        scanner.close();
    }
}
