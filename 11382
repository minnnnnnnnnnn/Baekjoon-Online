import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);
        long fir, sec, thi;

        fir = scanner.nextLong();
        sec = scanner.nextLong();
        thi = scanner.nextLong();

        System.out.println(fir + sec + thi);

        scanner.close();
    }
}
