import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        int t;

        t = scanner.nextInt();

        for(int i = 0; i < t; i++){
           for(int j = t - 1; j > i; j--){
               System.out.print(" ");
           }
           for(int k = 0; k <= i; k++){
               System.out.print("*");
           }
           System.out.println();
        }

        scanner.close();

    }
}
